{
  "name": "industrial-ui",
  "version": "0.2.0",
  "description": "Production-oriented library of styleless UI-components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "lib/index.js",
  "scripts": {
    "docs-dev": "cd ./docs && npm run dev",
    "docs-build": "cd ./docs && npm run build",
    "dev": "rollup -c -w",
    "export": "rollup -c",
    "build": "rollup -c && mkdir __sapper__ && cd ./docs && npm install && npm run build && mv ./__sapper__ ../",
    "start": "cd docs && npm install && npm start",
    "test": "jest lib",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanishMax/industrial-ui.git"
  },
  "keywords": [
    "industrial-ui",
    "ui-components",
    "ui",
    "ui-kit",
    "svelte",
    "javascript"
  ],
  "author": "VanishMax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VanishMax/industrial-ui/issues"
  },
  "homepage": "https://github.com/VanishMax/industrial-ui#readme",
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/svelte": "3.0.0",
    "babel-jest": "26.6.1",
    "jest": "26.6.1",
    "rollup": "2.32.1",
    "rollup-plugin-svelte": "6.1.0",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.29.4",
    "svelte-jester": "1.1.5"
  },
  "dependencies": {},
  "renovate": {
    "groupName": "all dependencies",
    "separateMajorMinor": false,
    "groupSlug": "all",
    "packageRules": [
      {
        "packagePatterns": [
          "*"
        ],
        "groupName": "all dependencies",
        "groupSlug": "all"
      }
    ],
    "lockFileMaintenance": {
      "enabled": false
    }
  }
}
