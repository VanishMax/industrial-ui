{
  "name": "industrial-ui",
  "version": "0.1.3",
  "description": "Industry-oriented library of unstyled UI-components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "lib/index.js",
  "scripts": {
    "docs-dev": "cd ./docs && npm run dev",
    "docs-build": "cd ./docs && npm run build",
    "dev": "rollup -c -w",
    "export": "rollup -c",
    "build": "rollup -c && mkdir __sapper__ && cd ./docs && npm install && npm run build && mv ./__sapper__ ../",
    "start": "cd docs && npm install && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanishMax/industrial-ui.git"
  },
  "keywords": [
    "industrial-ui",
    "ui-components",
    "ui",
    "ui-kit",
    "svelte",
    "javascript"
  ],
  "author": "VanishMax",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VanishMax/industrial-ui/issues"
  },
  "homepage": "https://github.com/VanishMax/industrial-ui#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "7.1.3",
    "rollup": "1.32.1",
    "rollup-plugin-svelte": "5.1.1",
    "rollup-plugin-terser": "5.3.0",
    "svelte": "3.18.1"
  },
  "dependencies": {},
  "renovate": {
    "extends": [
      "config:base"
    ],
    "packageRules": [
      {
        "updateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "automerge": true
      }
    ]
  }
}
